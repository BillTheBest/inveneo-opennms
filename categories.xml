<catinfo>
    <header>
        <rev>1.3</rev>
        <created>Wednesday, February 6, 2002 10:10:00 AM EST</created>
        <mstation>checkers</mstation>
    </header>
    <categorygroup>
        <name>WebConsole</name>
        <comment>Service Level Availability by Functional Group</comment>
        <common>
            <rule><![CDATA[IPADDR != '0.0.0.0']]></rule>
        </common>
        <categories>
            <category>
                <label><![CDATA[Ubiquiti Radios]]></label>
                <comment>Are Ubiquiti Radios in the Network pingable</comment>
                <normal>99.99</normal>
                <warning>97</warning>
                <service>ICMP</service>
                <rule><![CDATA[(nodeSysOID like '.1.3.6.1.4.1.10002.%')]]></rule>
            </category>
            <category>
                <label><![CDATA[Inveneo Hub Servers]]></label>
                <comment>Are Hub Servers pingable</comment>
                <normal>99.99</normal>
                <warning>97</warning>
                <service>ICMP</service>
                <rule><![CDATA[(nodeSysOID like '.1.3.6.1.4.1.34319.1.%')]]></rule>
            </category>
            <category>
                <label><![CDATA[VOIP]]></label>
                <comment>Is VOIP equipment pingable</comment>
                <normal>99.99</normal>
                <warning>97</warning>
                <service>ICMP</service>
                <rule><![CDATA[ipaddr IPLIKE 10.0.1.70-89]]></rule>
            </category>
            <category>
                <label><![CDATA[ISP Gateways]]></label>
                <comment>Ping test of gateways for all ISPs</comment>
                <normal>99.99</normal>
                <warning>97</warning>
                <service>ICMP</service>
		<rule><![CDATA[categoryName == 'ISP Gateways']]></rule>		
            </category>
	    <category>
                <label><![CDATA[ISP DNS Servers]]></label>
                <comment>Ping test for DNS Servers across all ISPs</comment>
                <normal>99.99</normal>
                <warning>97</warning>
                <service>ICMP</service>
		<rule><![CDATA[categoryName == 'DNS Servers']]></rule>		
	    </category>
        </categories>
    </categorygroup>
</catinfo>
